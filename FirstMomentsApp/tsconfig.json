{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@screens/*": [
        "./screens/*"
      ],
      "@services/*": [
        "./services/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@types/*": [
        "./types/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@store/*": [
        "./store/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@navigation/*": [
        "./navigation/*"
      ]
    },
    "noImplicitAny": true,
    "strictNullChecks": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
